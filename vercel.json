{
    "version": 2,
    "buildCommand": "npm install && npm run build",
    "outputDirectory": "build",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/backend/server.js"
        }
    ],
    "builds": [
      {
        "src": "./api/",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }